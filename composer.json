{
    "name": "phlexible/country-context-bundle",
    "description": "phlexible country context bundle",
    "license": "proprietary",
	"type": "symfony-bundle",
	"keywords": ["content", "country"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\CountryContextBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.3.0",
        "phlexible/phlexible": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.brainbits.net/repo/private/",
            "options": {
                "ssl": {
                    "allow_self_signed": "true"
                }
            }
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
