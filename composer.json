{
    "name": "phlexible/country-context-bundle",
    "description": "phlexible country context bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["content", "country"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\CountryContextBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.6.0",
        "phlexible/element-bundle": "^1.3.0",
        "phlexible/tree-bundle": "^1.3.0",
        "phlexible/element-renderer-bundle": "^1.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "symfony/http-foundation": "^2.8",
        "twig/twig": "^1.26",
        "phlexible/indexer-bundle": "^1.2.0",
        "phlexible/indexer-page-bundle": "^1.3.0",
        "matthiasnoback/symfony-config-test": "^2.0",
        "matthiasnoback/symfony-dependency-injection-test": "^1.0",
        "doctrine/orm": "^2.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
